package hilo3;

public class Actividad3 {

	
	public static void main(String[] args) {
		Hilo hilo1 = new Hilo();
		Hilo hilo2 = new Hilo();
		hilo2.setName("SUPER-HILO-DM2");
		hilo1.setPriority(5);
		hilo2.setPriority(6);
		
		hilo1.start();
		System.out.println("El nombre del hilo es " + hilo1.getName() + " y tiene la prioridad " + hilo1.getPriority());
		
		
		hilo2.start();
		System.out.println("Ahora el nombre del hilo es " + hilo1.getName() + " y tiene la prioridad " + hilo1.getPriority());
		
		System.out.println("Final del programa");
	}
	
	
	
}
